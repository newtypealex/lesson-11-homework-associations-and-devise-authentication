<h1>Article Page</h1>

<div>
<h2><%= @article.title %></h2>
<h3><%= @article.body %></h3>

<h3>by: <%= @article.author.try(:first_name)%></h3>

<h3>tags</h3>
 <ul>
   <% @article.tags.each do |tag| %>
     <li><%= tag.name %></li>
   <% end %>
 </ul>

</div>

<p><%= link_to 'EDIT', edit_article_path(@article) %></p>
<p><%= link_to 'INDEX PAGE', articles_path %></p>